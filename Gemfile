source "https://rubygems.org"

ruby "3.3.5"

#These are the direct (gem) dependencies for all the project operations

#the json gem helps with the fillJSON function
gem "json", "~>2.7.6"

#the spreadsheet gem helps read and write xls files
gem "spreadsheet", "~>1.3.3"

#the rubocop gem is a formatter, and is not essential to the code operation. 
#remove this line if it ever has problems installing
gem "rubocop"

#the rest of the gems are system defaults, but we include them here to be safe
gem "minitest", "~>5.25.2"